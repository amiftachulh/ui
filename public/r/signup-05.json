      "content": "import { styled } from \"styled-system/jsx\";\nimport { SignupForm } from \"@/registry/default/blocks/signup-05/components/signup-form\";\n\nexport default function SignupPage() {\n  return (\n    <styled.div\n      css={{\n        bg: \"bg\",\n        display: \"flex\",\n        minH: \"svh\",\n        flexDir: \"column\",\n        alignItems: \"center\",\n        justifyContent: \"center\",\n        gap: \"6\",\n        p: \"6\",\n        md: { p: \"10\" },\n      }}\n    >\n      <styled.div css={{ w: \"full\", maxW: \"sm\" }}>\n        <SignupForm />\n      </styled.div>\n    </styled.div>\n  );\n}\n"
